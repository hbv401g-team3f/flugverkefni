package models;

public class PassengerLuxuries {
	// PRIVATE VARIABLES
	private boolean wifiAvailable;
	private boolean mealsAvailable;
	private int priceInFlightPoints;
	private int flightPointsGained;
	
	// GET/SET METHODS
	private boolean isWifiAvailable() {
		return wifiAvailable;
	}
	private void setWifiAvailable(boolean wifiAvailable) {
		this.wifiAvailable = wifiAvailable;
	}
	private boolean isMealsAvailable() {
		return mealsAvailable;
	}
	private void setMealsAvailable(boolean mealsAvailable) {
		this.mealsAvailable = mealsAvailable;
	}
	private int getPriceInFlightPoints() {
		return priceInFlightPoints;
	}
	private void setPriceInFlightPoints(int priceInFlightPoints) {
		this.priceInFlightPoints = priceInFlightPoints;
	}
	private int getFlightPointsGained() {
		return flightPointsGained;
	}
	private void setFlightPointsGained(int flightPointsGained) {
		this.flightPointsGained = flightPointsGained;
	}
	
}
