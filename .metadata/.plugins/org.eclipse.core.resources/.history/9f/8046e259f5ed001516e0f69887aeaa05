package models;
import org.eclipse.swt.widgets.DateTime;

public class ConnectFlight {
	// PRIVATE VARIABLES
	private String connLoc;
	private DateTime connArrivalTime;
	private String connAirportId;
	private String connDepartTime;
	private int connGate;
	
	// GET/SET METHODS
	private String getConnLoc() {
		return connLoc;
	}
	private void setConnLoc(String connLoc) {
		this.connLoc = connLoc;
	}
	private DateTime getConnArrivalTime() {
		return connArrivalTime;
	}
	private void setConnArrivalTime(DateTime connArrivalTime) {
		this.connArrivalTime = connArrivalTime;
	}
	private String getConnAirportId() {
		return connAirportId;
	}
	private void setConnAirportId(String connAirportId) {
		this.connAirportId = connAirportId;
	}
	private String getConnDepartTime() {
		return connDepartTime;
	}
	private void setConnDepartTime(String connDepartTime) {
		this.connDepartTime = connDepartTime;
	}
	private int getConnGate() {
		return connGate;
	}
	private void setConnGate(int connGate) {
		this.connGate = connGate;
	}
		
}
